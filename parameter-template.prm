# Rename the file to vfp-equation.prm and
# set the parameters as needed

subsection Mesh
  set Number of refinements = 4
end

subsection Time stepping
  set Time step size = 7.8125e-3
  set Final time = 0.8
end

subsection Expansion
  set Expansion order = 2
end

subsection Finite element
  set Polynomial degree = 2
end

subsection Physical parameters
  set Scattering frequency = 1.
  set Particle energy = 1.
end